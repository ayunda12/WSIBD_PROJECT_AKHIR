	<queryString language="SQL">
		<![CDATA[SELECT
     tb_detailtransaksi.`jumlah`,
     tb_detailtransaksi.`kode_detail`,
     tb_detailtransaksi.`kode_transaksi`,
     tb_transaksi.`tgl_terima`,
     tb_transaksi.`tgl_selesai`,
     tb_transaksi.`kode_pelanggan`,
     tb_pelanggan.`nama_pelanggan`,
     tb_transaksi.`kode_pengguna`,
     tb_pengguna.`nama`,
     tb_detailtransaksi.`kode_paket`,
     tb_paketlaundry.`nama_paket`
FROM
     `tb_pelanggan` tb_pelanggan INNER JOIN `tb_transaksi` tb_transaksi ON tb_pelanggan.`kode_pelanggan` = tb_transaksi.`kode_pelanggan`
     INNER JOIN `tb_detailtransaksi` tb_detailtransaksi ON tb_transaksi.`kode_transaksi` = tb_detailtransaksi.`kode_transaksi`
     INNER JOIN `tb_pengguna` tb_pengguna ON tb_transaksi.`kode_pengguna` = tb_pengguna.`kode_pengguna`
     INNER JOIN `tb_paketlaundry` tb_paketlaundry ON tb_detailtransaksi.`kode_paket` = tb_paketlaundry.`kode_paket`]]>
	</queryString>
